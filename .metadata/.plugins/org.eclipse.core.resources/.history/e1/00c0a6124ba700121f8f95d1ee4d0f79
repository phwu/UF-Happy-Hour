package com.example.uf_happy_hour;


// Make Bar similar to Contacts Class

public class Bar {
	private String name;
	private String address;
	private int phoneNumber;
	private String venueType;
	private String hoursOpen;
	private String specials;
	private int ageReq;
	private int food;
	private String fullBar;
	
	public Bar() {
		this.name = "Error";
		this.address = "nil";
		this.phoneNumber = -1;
		this.venueType = "";
		this.hoursOpen = "5pm-7pm";
		this.specials = "None";
		this.ageReq = 21;
		this.food = 0;
		this.fullBar = "Beer/Wine only";
	}

	public void Bar(String name, String address, int phoneNumber, String venue, String hoursOpen, String specials, int ageReq, int food, String fullBar) {
		this.name = name;
		this.address = address;
		this.phoneNumber = phoneNumber;
		this.venueType = venue;
		this.hoursOpen = hoursOpen;
		this.specials = specials;
		this.ageReq = ageReq;
		this.food = food;
		this.fullBar = fullBar;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getAddress() {
		return this.address;
	}
	
	public void setAddress(String address) {
		this.address = address;
	}
	
	public int getPhoneNumber() {
		return this.phoneNumber;
	}
	
	public void setPhoneNumber(int number) {
		this.phoneNumber = number;
	}
	
	public String getVenue() {
		return this.venueType;
	}
	
	public void setVenue(String venue) {
		this.venueType = venue;
	}
	
	public String getHoursOpen() {
		return this.hoursOpen;
	}
	
	public void setHoursOpen(String hours) {
		this.hoursOpen = hours;
	}
	
	public String getSpecials() {
		return this.specials;
	}
	
	public int getAgeReq() {
		return this.ageReq;
	}
	
	public void setAgeReq(int age) {
		this.ageReq = age;
	}
	
	public int getFood() {
		return this.food;
	}
	
	public void setFood(int food) {
		this.food = food;
	}
	
	public String getFullBar() {
		return this.fullBar;
	}
	
	public void setFullBar(String fullBar) {
		this.fullBar = fullBar;
	}

}
